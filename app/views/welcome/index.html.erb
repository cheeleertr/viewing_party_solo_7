
<h1>Viewing Party</h1>

<% if session[:user_id] %>
   <h2>Existing Users</h2>
   <ul id="existing_users">
      <% @users.each do |user| %>
         <li> <%= user.email %> </li>
      <% end %>
   </ul>
<% else %>
   <%= link_to "Register as a User", new_user_path, class: "btn btn-lg btn-outline-secondary" %>
   <%= link_to "I already have an account", login_path, class: "btn btn-lg btn-outline-secondary" %>
<% end %>
